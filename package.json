    "main": "index.js",
    "scripts": {
        "dev": "nodemon app.js",
        "model-gen:history": "npx sequelize-cli model:generate --name History --attributes historyScore:integer",
        "model-gen:user": "npx sequelize-cli model:generate --name User --attributes Username:string,passwordUser:string",
        "model-gen:biodata": "npx sequelize-cli model:generate --name Biodata --attributes firstName:string,lastName:string,address:string,email:string",
        "migrate": "npx sequelize-cli db:migrate",
        "seed-gen:biodata": "npx sequelize-cli seed:generate --name biodata",
        "seed-gen:history": "npx sequelize-cli seed:generate --name history",
        "seed-gen:user": "npx sequelize-cli seed:generate --name user",
        "seed": "npx sequelize-cli db:seed:all",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
