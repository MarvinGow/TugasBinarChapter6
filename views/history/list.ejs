<!DOCTYPE html>
<html>

<head>
    <title>History Data</title>
    <link href="/css/style.css" rel="stylesheet" type="text/css">
</head>

<body>
    <div class="masthead">
        <h1>History Data</h1>
        <nav>
            <%- include('../nav/navbar') %>
                <a href="/history/add">Add history</a>
        </nav>
    </div>
    <div class="form-center">
        <table>
            <tr>
                <th>Id</th>
                <th>History Score</th>
                <th>Action1</th>
            </tr>
            <% if (history !== null) { %>
                <% history.forEach(td => { %>
                    <tr>
                        <td>
                            <%=td.id%>
                        </td>
                        <td>
                            <%=td.historyScore%>
                        </td>
                        <td>
                            <a href="/history/edit/<%=td.id%>">Edit</a> |
                            <a href="/history/delete/<%=td.id%>" onclick="return confirm('delete this history?')">Delete</a>
                        </td>
                    </tr>
                    <% }) %>
                        <% } %>
                            <table>
    </div>

</body>

</html>